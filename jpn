import geopandas as gpd

world_gdf = gpd.read_file(gpd.datasets.get_path('naturalearth_lowres'))
arable = pd.read_csv('arable.csv')

young_pop = pd.read_csv('0014.csv')
young_pop.head()

geo_young = pd.merge(world_gdf, young_pop, left_on='iso_a3', right_on='Country Code')
geo_young = geo_young[['geometry', 'name','continent', '2021']]

geo_young.explore('2021')
